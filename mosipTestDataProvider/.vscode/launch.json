    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch CertificateGenerator",
            "request": "launch",
            "mainClass": "org.mosip.dataprovider.test.partnerManagement.CertificateGenerator",
            "projectName": "dataprovider"
        },
        {
            "type": "java",
            "name": "Launch CertificateUploader",
            "request": "launch",
            "mainClass": "org.mosip.dataprovider.test.CertificateUploader",
            "projectName": "dataprovider"
        },
        {
            "type": "java",
            "name": "Launch MosipMasterData",
            "request": "launch",
            "mainClass": "org.mosip.dataprovider.preparation.MosipMasterData",
            "projectName": "dataprovider"
        },
        {
            "type": "java",
            "request": "attach",
            "name": "Java",
            "port": 5005,
            "sourceRoots": []
        },
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        }
    ]
}